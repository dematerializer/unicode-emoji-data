{
  "parser": "babel-eslint",
  "extends": "airbnb-base",
  "env": {
    "node": true,
    "mocha": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
  },
  "rules": {
    "import/no-extraneous-dependencies": 0,
    "import/no-unresolved": 2,
    "comma-dangle": [2, "always-multiline"],
    "no-tabs": 0,
    "indent": [2, "tab", { "SwitchCase": 1 }],
    "max-len": 0,
    "prefer-template": 2,
    "no-use-before-define": 0,
    "newline-per-chained-call": 0,
    "arrow-body-style": [2, "as-needed"]
  }
}
