{
  "name": "unicode-emoji-data",
  "version": "0.0.0",
  "private": true,
  "description": "unicode compliant emoji data",
  "author": "Andreas Tietz https://github.com/dematerializer",
  "main": "dist/index.js",
  "files": ["dist"],
  "scripts": {
    "start": "npm run build:emoji",
    "build:emoji": "NODE_ENV=dev node build-emoji-data.js",
    "test": "NODE_ENV=test nyc --reporter=lcov --reporter=text mocha --require ./test-env --compilers js:babel-core/register src/**/*.test.js",
    "posttest": "NODE_ENV=test npm run lint",
    "lint": "NODE_ENV=test eslint --ignore-path .gitignore .",
    "prepublish": "npm run test && npm run build:sources && npm run copy:json",
    "build:sources": "mkdir -p dist; babel src/index.js -o dist/index.js; babel src/expand-emoji-data.js -o dist/expand-emoji-data.js",
    "copy:json": "mkdir -p dist; cp src/emoji-data-*.json dist"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.0.0",
    "babel-plugin-istanbul": "^2.0.3",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.16.3",
    "chai": "^3.5.0",
    "cheerio": "^0.22.0",
    "co": "^4.6.0",
    "eslint": "^3.9.1",
    "eslint-config-airbnb-base": "^9.0.0",
    "eslint-plugin-import": "^2.0.1",
    "fetch-mock": "^5.5.0",
    "isomorphic-fetch": "^2.2.1",
    "left-pad": "^1.1.3",
    "log-update": "^1.0.2",
    "mocha": "^3.1.2",
    "nyc": "^8.3.2",
    "punycode": "^2.0.0"
  },
  "dependencies": {
    "punycode": "^2.0.1"
  }
}
